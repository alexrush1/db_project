CREATE TABLE  "PLANE"
   (	"PLANE_ID" NUMBER NOT NULL ENABLE,
	"AIRCRAFT_TYPE" VARCHAR2(15) NOT NULL ENABLE,
	"NUM_OF_REPAIRS" NUMBER NOT NULL ENABLE,
	"LAST_REPAIR" DATE,
	"AGE" NUMBER NOT NULL ENABLE,
	"FLIGHT_HOURS" NUMBER NOT NULL ENABLE,
	 CONSTRAINT "PLANE_PK" PRIMARY KEY ("PLANE_ID") ENABLE
   ) ;

CREATE sequence "PLANE_SEQ";

CREATE OR REPLACE TRIGGER  "BI_PLANE"
  before insert on "PLANE"
  for each row
begin
  (if :NEW."PLANE_ID" is null then
    ( select "PLANE_SEQ".nextval into :NEW."PLANE_ID" from dual; )
  end if;)
end;

ALTER TRIGGER  "BI_PLANE" DISABLE;

CREATE TABLE  "FLIGHT_SCHEDULE"
   (	"FLIGHT_ID" NUMBER NOT NULL ENABLE,
	"PLANE_ID" NUMBER NOT NULL ENABLE,
	"AIRCRAFT_TYPE" VARCHAR2(15) NOT NULL ENABLE,
	"FLIGHT_NUMBER" VARCHAR2(10) NOT NULL ENABLE,
	"DEPARTURE_DAYS" VARCHAR2(15) NOT NULL ENABLE,
	"DEPARTURE_TIME" VARCHAR2(7) NOT NULL ENABLE,
	"ARRIVAL_TIME" VARCHAR2(7) NOT NULL ENABLE,
	"DEPARTURE" VARCHAR2(7) NOT NULL ENABLE,
	"ARRIVAL" VARCHAR2(7) NOT NULL ENABLE,
	"TICKET_PRICE" NUMBER NOT NULL ENABLE,
	 CONSTRAINT "FLIGHT_SCHEDULE_PK" PRIMARY KEY ("FLIGHT_ID") ENABLE
   );

ALTER TABLE  "FLIGHT_SCHEDULE" ADD CONSTRAINT
"FLIGHT_SCHEDULE_FK"
FOREIGN KEY ("PLANE_ID")
REFERENCES  "PLANE" ("PLANE_ID")
ON DELETE SET NULL ENABLE;

CREATE sequence "FLIGHT_SCHEDULE_SEQ";

CREATE OR REPLACE TRIGGER  "BI_FLIGHT_SCHEDULE"
  before insert on "FLIGHT_SCHEDULE"
  for each row
begin
  (if :NEW."FLIGHT_ID" is null then
    (select "FLIGHT_SCHEDULE_SEQ".nextval into :NEW."FLIGHT_ID" from dual;)
  end if;)
end;

ALTER TRIGGER  "BI_FLIGHT_SCHEDULE" DISABLE;

CREATE table "EMPLOYEES_TYPE" (
    "EMPLOYEE_ID" NUMBER NOT NULL,
    "TYPE"        NUMBER(2) NOT NULL,
    constraint  "EMPLOYEES_TYPE_PK" primary key ("EMPLOYEE_ID")
);

CREATE sequence "EMPLOYEES_TYPE_SEQ";

CREATE trigger "BI_EMPLOYEES_TYPE"
  before insert on "EMPLOYEES_TYPE"
  for each row
begin
  (if :NEW."EMPLOYEE_ID" is null then
    (select "EMPLOYEES_TYPE_SEQ".nextval into :NEW."EMPLOYEE_ID" from dual;)
  end if;)
end;

ALTER TRIGGER  "BI_EMPLOYEES_TYPE" DISABLE;

CREATE table "PLANE_EMPLOYEES" (
    "PLANE_ID"    NUMBER NOT NULL,
    "EMPLOYEE_ID" NUMBER NOT NULL,
    "POSITION"    NUMBER NOT NULL,
    constraint  "PLANE_EMPLOYEES_PK" primary key ("EMPLOYEE_ID")
);

CREATE sequence "PLANE_EMPLOYEES_SEQ";

CREATE trigger "BI_PLANE_EMPLOYEES"
  before insert on "PLANE_EMPLOYEES"
  for each row
begin
  (if :NEW."EMPLOYEE_ID" is null then
    (select "PLANE_EMPLOYEES_SEQ".nextval into :NEW."EMPLOYEE_ID" from dual;)
  end if;)
end;

ALTER TRIGGER  "BI_PLANE_EMPLOYEES" DISABLE;

CREATE TABLE  "EMPLOYEES"
   (	"EMPLOYEE_ID" NUMBER,
	"FIRST_NAME" VARCHAR2(20) NOT NULL ENABLE,
	"LAST_NAME" VARCHAR2(20) NOT NULL ENABLE,
	"AGE" NUMBER NOT NULL ENABLE,
	"SEX" NUMBER(1,0) NOT NULL ENABLE,
	"CHILDREN" NUMBER(2,0),
	"DEPARTMENT_ID" NUMBER NOT NULL ENABLE,
	 CONSTRAINT "EMPLOYEES_PK" PRIMARY KEY ("EMPLOYEE_ID") ENABLE
   ) ;

CREATE sequence "EMPLOYEES_SEQ";

CREATE OR REPLACE TRIGGER  "BI_EMPLOYEES"
  before insert on "EMPLOYEES"
  for each row
begin
  (if :NEW."EMPLOYEE_ID" is null then
    (select "EMPLOYEES_SEQ".nextval into :NEW."EMPLOYEE_ID" from dual;)
  end if;)
end;

ALTER TRIGGER  "BI_EMPLOYEES" DISABLE;

CREATE table "MED_INSPECTION" (
    "ID"              NUMBER NOT NULL,
    "EMPLOYEE_ID"     NUMBER NOT NULL,
    "INSPECTION_DATE" DATE NOT NULL,
    "STATUS"          NUMBER(1) NOT NULL,
    constraint  "MED_INSPECTION_PK" primary key ("ID")
);

CREATE sequence "MED_INSPECTION_SEQ";

CREATE trigger "BI_MED_INSPECTION"
  before insert on "MED_INSPECTION"
  for each row
begin
  (if :NEW."ID" is null then
    (select "MED_INSPECTION_SEQ".nextval into :NEW."ID" from dual;)
  end if;)
end;

ALTER TRIGGER  "BI_MED_INSPECTION" DISABLE;

ALTER TABLE "MED_INSPECTION" ADD CONSTRAINT "MED_INSPECTION_FK"
FOREIGN KEY ("EMPLOYEE_ID")
REFERENCES "EMPLOYEES" ("EMPLOYEE_ID")
ON DELETE CASCADE;

CREATE table "PASSENGERS" (
    "ID"            NUMBER NOT NULL,
    "FIRST_NAME"    VARCHAR2(20) NOT NULL,
    "LAST_NAME"     VARCHAR2(20) NOT NULL,
    "DOCUMENT_ID"   NUMBER NOT NULL,
    "FLIGHT_NUMBER" VARCHAR2(10) NOT NULL,
    "STATUS"        NUMBER NOT NULL,
    constraint  "PASSENGERS_PK" primary key ("ID")
);

CREATE sequence "PASSENGERS_SEQ";

CREATE trigger "BI_PASSENGERS"
  before insert on "PASSENGERS"
  for each row
begin
  (if :NEW."ID" is null then
    (select "PASSENGERS_SEQ".nextval into :NEW."ID" from dual;)
  end if;)
end;

ALTER TRIGGER "BI_PASSENGERS" DISABLE;

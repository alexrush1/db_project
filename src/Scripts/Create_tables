CREATE TABLE "FLIGHT_SCHEDULE"
(	"AIRCRAFT_ID" NUMBER NOT NULL ENABLE,
    "AIRCRAFT_TYPE" VARCHAR2(15) NOT NULL ENABLE,
     "FLIGHT_NUMBER" VARCHAR2(10) NOT NULL ENABLE,
     "DEPARTURE_DAYS" VARCHAR2(15) NOT NULL ENABLE,
     "DEPARTURE_TIME" VARCHAR2(7) NOT NULL ENABLE,
     "ARRIVAL_TIME" VARCHAR2(7) NOT NULL ENABLE,
     "DEPARTURE" VARCHAR2(7) NOT NULL ENABLE,
     "ARRIVAL" VARCHAR2(7) NOT NULL ENABLE,
     "TICKET_PRICE" NUMBER NOT NULL ENABLE,
     CONSTRAINT "FLIGHT_SCHEDULE_PK" PRIMARY KEY ("FLIGHT_NUMBER") ENABLE
);